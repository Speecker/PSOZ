 <recipe name="Tank: XP Bottle, emptying" required="true">
 <tanking type="EMPTY" logic="XPBOTTLE">
 <input name="minecraft:experience_bottle"/>
 <fluid name="xpjuice" amount="1"/><!-- for logic=XPBOTTLE amount is a factor to the vanilla amount -->
 </tanking>
 </recipe>
